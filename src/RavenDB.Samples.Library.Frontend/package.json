		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/dompurify": "^3.0.5",
		"@types/node": "^24.10.1",
		"@vitest/browser": "^4.0.14",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"globals": "^16.5.0",
		"playwright": "^1.57.0",
		"prettier": "^3.7.3",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.45.2",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.0",
